[program:laravel-worker-default]
process_name=%(program_name)s_%(process_num)02d
command=php {{working_dir}}/artisan queue:work --queue=local-default --timeout 86400
autostart=true
autorestart=true
user=vagrant
numprocs=2
redirect_stderr=true
stdout_logfile={{working_dir}}/storage/logs/laravel-worker-default.log
